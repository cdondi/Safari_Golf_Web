<%--
Default skin template. The following skins are provided as examples only.

1. Named control skin. The SkinId should be uniquely defined because
   duplicate SkinId's per control type are not allowed in the same theme.

<asp:GridView runat="server" SkinId="gridviewSkin" BackColor="White" >
   <AlternatingRowStyle BackColor="Blue" />
</asp:GridView>

2. Default skin. The SkinId is not defined. Only one default 
   control skin per control type is allowed in the same theme.

<asp:Image runat="server" ImageUrl="~/images/image1.jpg" />
--%>
<asp:WebPartZone runat="server" BorderColor="#CCCCCC" Font-Names="Verdana"
    Padding="6" WebPartVerbRenderMode="TitleBar" Width="100%">
    <PartChromeStyle BackColor="#f0fff0" BorderColor="#E2DED6" Font-Names="Verdana" ForeColor="Black" />
    <MenuLabelHoverStyle ForeColor="#E2DED6" />
    <EmptyZoneTextStyle Font-Size="0.8em" />
    <MenuLabelStyle ForeColor="Black" />
    <MenuVerbHoverStyle BackColor="#f0fff0" BorderColor="#CCCCCC" BorderStyle="Solid"
        BorderWidth="1px" ForeColor="#333333" />
    <HeaderStyle Font-Size="0.7em" ForeColor="Black" HorizontalAlign="Center" />
    <MenuVerbStyle BorderColor="#f0fff0" BorderStyle="Solid" BorderWidth="1px" ForeColor="Black" />
    <PartStyle Font-Size="0.8em" ForeColor="Black" />
    <TitleBarVerbStyle Font-Size="1.0em" Font-Underline="False" ForeColor="Black" />
    <MenuPopupStyle BackColor="#f0fff0" BorderColor="#CCCCCC" BorderWidth="1px" Font-Names="Verdana"
        Font-Size="0.6em" />
    <PartTitleStyle BackColor="#f0fff0" Font-Bold="True" Font-Size="0.1em" ForeColor="#f0fff0" />
</asp:WebPartZone>

<asp:Menu runat="server" Orientation="Horizontal"
    CssClass="navtop" Width="100%">
    <DynamicHoverStyle BackColor="#faf0e6" />
    <DynamicMenuItemStyle BackColor="#faf0e6" />
</asp:Menu>                       