                        <Columns>
                                                                
                            <asp:BoundField DataField = "Ordinal" HeaderText = "Hole Number" />
                            <asp:BoundField DataField = "Yardage" HeaderText = "Yardage" />
                            <asp:BoundField DataField = "Par" HeaderText = "Par" />
                            <asp:BoundField DataField = "Handicap" HeaderText = "Hole Handicap"  /> 

                            </Columns>
                        </asp:GridView>
                          
                
